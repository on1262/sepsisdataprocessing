{
    "dataset":{
        "remove_rule":{
            "min_cover_rate": 0.5,
            "version1": {
                "remove_empty": true
            },
            "version2":{
                "remove_empty": true,
                "target_id": ["220224", "223835"],
                "duration_minmax": [2, 96],
                "check_sepsis_time": [-10, 30],
                "min_feature_missrate": 0
            }
        },
        "k-fold":5,
        "alignment_key_id": ["220224", "223835"],
        "additional_features":["data_source", "PPD", "MAP", "shock_index", "PF_ratio"],
        "#high_correlation:": ["Height (Inches)", "225677", "220545", "226730", "226531", "Weight (Lbs)", "227467", "220339", "227443", "220587"],
        "version":{
            "icu_version":{
                "data_source": ["icu"],
                "fill_missvalue":"avg",
                "feature_limit": [],
                "forbidden_feas":["Height (Inches)", "225677", "220545", "226730", "226531", "Weight (Lbs)", "227467", "220339", "227443", "220587", "data_source"]
            }
        },
        "value_clip":{
            "Inspired O2 Fraction": {"min": 21, "max":100},
            "Arterial O2 pressure": {"min": 0, "max": 500}
        }
    },
    "model":{
        "feature_explore":{
            "dataset_version":"icu_version",
            "plot_samples":{
                "enabled": true,
                "n_sample": 50,
                "features": ["Arterial O2 pressure", "Inspired O2 Fraction"]
            },
            "plot_time_series":{
                "enabled": true,
                "names": ["PF_ratio"],
                "n_sample": 400,
                "n_per_plots": 40
            },
            "correlation":{
                "enabled":true,
                "target": "PF_ratio"
            },
            "miss_mat":true,
            "first_ards_time":true,
            "feature_count":true,
            "generate_report":true,
            "report_params":{
                "basic":true,
                "dynamic_dist":true,
                "static_dist":true,
                "hit_table": {
                    "enabled": true,
                    "min_thres": 0.5
                },
                "global_missrate":true
            }
        },
        "nearest_4cls":{
            "dataset_version":"icu_version",
            "window":16,
            "centers": [50, 150, 250, 350],
            "class_names": ["Severe", "Moderate", "Mild", "No_ARDS"],
            "smoothing_band":50
        }
    }
}